<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="category_checker" model="ir.cron">
            <field name="name">Sincroniza categorias</field>
            <field name="model_id" ref="model_product_public_category"></field>
            <field name="state">code</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
